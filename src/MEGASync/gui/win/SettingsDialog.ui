#wTabHeader QToolButton:checked
{
background-color: #FFFFFF;
border-bottom: 3px solid #0078D7;
}

#bChangePasswordPic, #bRecoveryKeyPic
{
border: none;
}

#lSyncType
{
font-weight: bold;
font-size: 15px;
}

#lSyncText
{
font-style: italic;
}

#lHeader
{
background-color: white;
border: none;
}

/* Syncs tab */
#wSyncsTable
{
background: white;
border: 1px solid #D8D8D8;
}

#wSyncsTable QHeaderView
{
background: transparent;
}

#wSyncsTable QHeaderView::section
{
border-style: none;
color: #4A607A;
background: white;
}

#wSyncsTable QHeaderView::section:horizontal
{
border-right: 1px solid #D8D8D8;
padding-left: 10px;
}

#wSyncsTable QTableView
{
border: none;
background: transparent;
}
#wSyncsTable QTableView::item
{
border-right: 1px solid #D8D8D8;
}

#wSyncsTable QTableWidget::item:selected
{
background-color: rgba(0,120,215,0.8);
border-right: 1px solid #2b8bd7;
}

#tSyncs QMenu
{
border: 1px solid #B8B8B8;
border-radius: 8px;
background: #ffffff;
padding-top: 8px;
padding-bottom: 8px;
}

#tSyncs QMenu::separator
{
height: 1px;
margin: 0px 8px 0px 8px;
background-color: rgba(0, 0, 0, 0.1);
}

#tSyncs QMenu::item
{
background-color: transparent;
border-radius: 8px;
}

#lAccountType, #lName
{
font-size: 13px;
}

#lBandwidthFree
{
color: #0078D7;
}

#lAccountType
{
text-align: left;
border: none;
padding: 0px;
margin: 0px;
}

#lUploadRateLimit, #lDownloadRateLimit, #lProxySettings
{
font-weight: bold;
}

#cbSleepMode 
{
margin-top: 4px;
}

#lSleepModeWarning
{
font-size: 10px;
line-height: 16px;
font-weight: 400;
color: #505862;
}</string>
